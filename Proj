 const code = "some code";
 const index = this.currentNotebookEditor.notebookPanel.content.activeCellIndex + 1;
    this.currentNotebookEditor.notebookPanel.content.model.cells.insert(
        index,
        new CodeCellModel({
            cell: {
                source: code,
                cell_type: 'code',
                metadata: {},
            },
        })
    );
